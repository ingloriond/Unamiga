FUNCTION altsyncram_4va1 (aclr0, address_a[2..0], address_b[2..0], clock0, clocken0, data_a[4..0], wren_a)
RETURNS ( q_b[4..0]);
FUNCTION cntr_apf (clk_en, clock)
RETURNS ( q[2..0]);
FUNCTION cntr_09h (aset, clk_en, clock, cnt_en)
RETURNS ( cout);

--synthesis_resources = lut 6 M9K 1 reg 7 
SUBDESIGN shift_taps_1jm
( 
	aclr	:	input;
	clock	:	input;
	shiftin[4..0]	:	input;
	shiftout[4..0]	:	output;
	taps[4..0]	:	output;
) 
VARIABLE 
	altsyncram2 : altsyncram_4va1;
	dffe4 : dffe;
	cntr1 : cntr_apf;
	cntr3 : cntr_09h;
	clken	: NODE;

BEGIN 
	altsyncram2.aclr0 = dffe4.q;
	altsyncram2.address_a[] = cntr1.q[];
	altsyncram2.address_b[] = cntr1.q[];
	altsyncram2.clock0 = clock;
	altsyncram2.clocken0 = clken;
	altsyncram2.data_a[] = ( shiftin[]);
	altsyncram2.wren_a = B"1";
	dffe4.clk = clock;
	dffe4.d = (! cntr3.cout);
	dffe4.ena = clken;
	dffe4.prn = (! aclr);
	cntr1.clk_en = clken;
	cntr1.clock = clock;
	cntr3.aset = aclr;
	cntr3.clk_en = clken;
	cntr3.clock = clock;
	cntr3.cnt_en = (! cntr3.cout);
	clken = VCC;
	shiftout[4..0] = altsyncram2.q_b[4..0];
	taps[] = altsyncram2.q_b[];
END;
